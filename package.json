{
  "name": "venuu-events",
  "version": "1.0.0",
  "description": "Venuu - Discover Amazing Events in Iceland",
  "main": "index.html",
  "scripts": {
    "build-css": "tailwindcss -i ./src/input.css -o ./css/output.css --watch",
    "build-css-prod": "tailwindcss -i ./src/input.css -o ./css/output.css --minify",
    "build-ts": "tsc",
    "build-ts-watch": "tsc --watch",
    "dev": "npm run build-ts && npm run build-css",
    "dev:watch": "concurrently \"npm run build-ts-watch\" \"npm run build-css\"",
    "build": "npm run build-ts && npm run build-css-prod"
  },
  "keywords": [
    "events",
    "iceland",
    "tailwind",
    "css",
    "typescript"
  ],
  "author": "Venuu Team",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "lottie-web": "^5.13.0"
  }
}
